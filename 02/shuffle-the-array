/**
 * @param {number[]} nums
 * @param {number} n
 * @return {number[]}
 */
let shuffle = function(nums, n) {
   let nums2=[];
   let final=[];
    for(let i=n;i<nums.length;i++){
         nums2.push(nums[i]);
    }
    for(let i=0;i<n;i++){
        final.push(nums[i]);
        final.push(nums2[i]);
    }
    return final;
};
